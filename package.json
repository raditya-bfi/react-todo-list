{
  "name": "raditya-react-typescript-boilerplate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "local": "rimraf dev-dist && vite --force --mode local",
    "dev": "rimraf dev-dist && vite --force --mode dev",
    "sit": "rimraf dev-dist && vite --force --mode sit",
    "uat": "rimraf dev-dist && vite --force --mode uat",
    "build": "rimraf build && tsc && vite build",
    "build:dev": "rimraf build && tsc && vite build --mode dev",
    "build:sit": "rimraf build && tsc && vite build --mode sit",
    "build:uat": "rimraf build && tsc && vite build --mode uat",
    "build:production": "rimraf build && tsc && vite build --mode production",
    "checkTs": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "npx eslint@8.57.0 src",
    "lint:fix": "yarn run lint --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn run prettier --write",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "test": "jest --config scripts/jest.config.js",
    "test:coverage": "jest --config scripts/jest.config.js --collectCoverage"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@testing-library/dom": ">=7.21.4",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.3.0",
    "react-svg": "^15.1.10",
    "regenerator-runtime": "^0.13.9",
    "workbox-build": "^6.5.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "@types/loadable__component": "^5.13.5",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jest": "^26.6.3",
    "postcss": "^8.4.39",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^26.5.4",
    "typescript": "^4.6.3",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "packageManager": "yarn@4.0.2"
}
